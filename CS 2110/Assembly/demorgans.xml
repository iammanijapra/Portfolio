<?xml version="1.0"?>
<test-suite>    
    <test-case>
        <name>8 | 7 = 15</name>
        <has-max-executions>1</has-max-executions>
        <max-executions>1000000</max-executions>
        <randomize>1</randomize>
        <input>
            <test-value><address>A</address><value>8</value></test-value>
            <test-value><address>B</address><value>7</value></test-value>
        </input>
        <output>
            <test-value><address>ANSWER</address><value>15</value></test-value>
        </output>
    </test-case>
    <test-case>
        <name>42 | 10 = 42</name>
        <has-max-executions>1</has-max-executions>
        <max-executions>1000000</max-executions>
        <randomize>1</randomize>
        <input>
            <test-value><address>A</address><value>42</value></test-value>
            <test-value><address>B</address><value>10</value></test-value>
        </input>
        <output>
            <test-value><address>ANSWER</address><value>42</value></test-value>
        </output>
    </test-case>
</test-suite>
